prime=[2,3,5,7]
ans=0
k=int(input())
for i in range(0,2**k):
    a=[0]*k
    x,j=i,0
    while x>0:
        a[j]=x%2
        x//=2
        j+=1
    Sum=1
    flag=False
    for j in range(1,k):
        if a[j]==a[j-1]:
            Sum+=1
        else :
            if not (Sum in prime):
                flag=True
                break
            Sum=1
    if not (Sum in prime):
        flag=True
    if flag==False:
        ans+=1
print(ans)
